
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <meta name="description" content="egitmen paneli" />
    <meta name="author" content="Mehmet Arslan" />
    <title>Fivem Yönetim Paneli</title>

    <link href="~/Content/Panel/css/styles.css" rel="stylesheet" />
    <link href="~/Content/Site.css" rel="stylesheet" />
    <!-- Google font-->
    <link href="https://fonts.googleapis.com/css?family=Ubuntu:400,500,700" rel="stylesheet">

    <!-- themify -->
    <link rel="stylesheet" type="text/css" href="~/Content/login/icon/themify-icons/themify-icons.css">

    <!-- iconfont -->
    <link rel="stylesheet" type="text/css" href="~/Content/login/icon/icofont/css/icofont.css">

    <!-- simple line icon -->
    <link rel="stylesheet" type="text/css" href="~/Content/login/icon/simple-line-icons/css/simple-line-icons.css">

    <!-- Required Fremwork -->
    <link rel="stylesheet" type="text/css" href="~/Content/login/plugins/bootstrap/css/bootstrap.min.css">

    <!-- Chartlist chart css -->
    <link rel="stylesheet" href="~/Content/login/plugins/chartist/dist/chartist.css" type="text/css" media="all">

    <!-- Weather css -->
    <link href="~/Content/login/css/svg-weather.css" rel="stylesheet">


    <!-- Style.css -->
    <link rel="stylesheet" type="text/css" href="~/Content/login/css/main.css">

    <!-- Responsive.css-->
    <link rel="stylesheet" type="text/css" href="~/Content/login/css/responsive.css">

</head>
<body>
    <nav class="sb-topnav navbar navbar-expand navbar-dark bg-dark">
        <a class="navbar-brand" href="~/Login/EgitmenDashboard">Eğitmen Yönetim Paneli</a>
        <button class="btn btn-link btn-sm order-1 order-lg-0" id="sidebarToggle" href="#"><i class="fas fa-bars"></i></button>
        <!-- Navbar Search-->
        <form class="d-none d-md-inline-block form-inline ml-auto mr-0 mr-md-3 my-2 my-md-0">
            <div class="input-group">
                <div class="input-group-append">
                </div>
            </div>
        </form>
        <!-- Navbar-->
        <ul class="navbar-nav ml-auto ml-md-0">
            <li class="nav-item dropdown">
                <a class="nav-link dropdown-toggle" id="userDropdown" href="#" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"><i class="fas fa-user fa-fw"></i></a>
                <div class="dropdown-menu dropdown-menu-right" aria-labelledby="userDropdown">
                    <div class="dropdown-divider"></div>
                    <a class="dropdown-item" href="~/Account/Index">Ayarlar</a>
                    <a class="dropdown-item" href="~/Admin/Cikis">Çıkış Yap</a>
                </div>
                </a>
        </ul>
    </nav>
    <div id="layoutSidenav">
        <div id="layoutSidenav_nav">
            <nav class="sb-sidenav accordion sb-sidenav-dark" id="sidenavAccordion">
                <div class="sb-sidenav-menu">
                    <div class="nav">
                        <div class="sb-sidenav-menu-heading"></div>
                        <a class="nav-link" href="~/Login/EgitmenDashboard">
                            <div class="sb-nav-link-icon"><i class="fas fa-home"></i></div>
                            Anasayfa
                        </a>
                        <a class="nav-link" href="~/Course/CourseList">
                            <div class="sb-nav-link-icon"><i class="fa fa-list-ol"></i></div>
                            Eğitim İşlemleri
                        </a>
                        <a class="nav-link" href="~/Course/StudentList">
                            <div class="sb-nav-link-icon"><i class="fa fa-file-archive-o"></i></div>
                            Öğrenci İşlemleri
                        </a>

                        


                    </div>
                </div>
                <div class="sb-sidenav-footer">
                    <div class="small">Eğitmen Yönetim Paneli</div>
                </div>
            </nav>
        </div>

        <div id="layoutSidenav_content">
            <!--Container başlangıcı-->
            <div class="container-fluid">
                @RenderBody()
                @RenderSection("scripts", required: false)
            </div>
            <footer class="py-4 bg-light mt-auto">
                <div class="container-fluid">
                    <div class="d-flex align-items-center justify-content-between small">
                        <div class="text-muted">Copyright (c) 2021 Eğitmen Yönetim Paneli </div>
                        <div>
                            <a href="#">Gizlilik Politikası</a>
                            &middot;
                            <a href="#">Şartlar ve Koşullar</a>
                        </div>
                    </div>
                </div>
            </footer>
        </div>
    </div>
    <script src="https://kit.fontawesome.com/b0f80abb98.js" crossorigin="anonymous"></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/Content/Panel/js/scripts.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/Chart.js/2.8.0/Chart.min.js" crossorigin="anonymous"></script>
    <script src="~/Content/Panel/~/Content/login/demo/chart-area-demo.js"></script>
    <script src="~/Content/Panel/~/Content/login/demo/chart-bar-demo.js"></script>
    <script src="~/Content/Panel/~/Content/login/demo/chart-pie-demo.js"></script>
</body>
</html>