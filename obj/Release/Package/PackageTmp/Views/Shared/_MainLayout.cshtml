<!-- body -->

<body class="main-layout">
    <!-- loader  -->
    <div class="loader_bg">
        <div class="loader"><img src="~/Content/images/loading.gif" alt="#" /></div>
    </div>
    <!-- end loader -->
    <!-- header -->
    <header>
        <!-- header inner -->
        <div class="header-top">
            <div class="header">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-xl-2 col-lg-4 col-md-4 col-sm-3 col logo_section">
                            <div class="full">
                                <div class="center-desk">
                                    <div class="logo">
                                        <a href="~/Default/Index"><img src="~/Content/images/edulogo.png" alt="#" /></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="col-xl-10 col-lg-8 col-md-8 col-sm-9">
                            <div class="header_information">
                                <div class="menu-area">
                                    <div class="limit-box">
                                        <nav class="main-menu ">
                                            <ul class="menu-area-main">
                                                <li class="active"> <a href="~/Default/Index">Anasayfa</a> </li>
                                                <li> <a href="~/Course/Index">Kurslar </a> </li>
                                                <li> <a href="#contact">İletişim</a> </li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>
                                <div class="mean-last">
                                    @Html.ActionLink("Giriş Yap | Kayıt Ol", "Index", "Login")
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!-- end header inner -->

            @RenderBody()
            <!--  footer -->
            <footr>
                <div class="footer ">
                    <div class="container">
                        <div class="row">

                            <div class="col-md-12">
                                <form class="news">
                                    <input class="newslatter" placeholder="Email" type="text" name=" Email">
                                    <button class="submit">Abone Ol</button>
                                </form>
                            </div>
                            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 ">
                                <div class="row">
                                    <div class="col-xl-3 col-lg-3 col-md-6 col-sm-6 ">
                                        <div class="address">
                                            <h3>İletişim </h3>
                                            <ul class="loca">
                                                <li>
                                                    <a href="#"><img src="~/Content/icon/loc.png" alt="#" /></a>IIBF
                                                    <br>Cumhuriyet Üniversitesi
                                                </li>
                                                <li>
                                                    <a href="#"><img src="~/Content/icon/email.png" alt="#" /></a>demo@cumhuriyet.edu.tr
                                                </li>
                                                <li>
                                                    <a href="#"><img src="~/Content/icon/call.png" alt="#" /></a>+12586954775
                                                </li>
                                            </ul>
                                            <ul class="social_link">
                                                <li><a href="#"><img src="~/Content/icon/fb.png"></a></li>
                                                <li><a href="#"><img src="~/Content/icon/tw.png"></a></li>
                                                <li><a href="#"><img src="~/Content/icon/lin(2).png"></a></li>
                                                <li><a href="#"><img src="~/Content/icon/instagram.png"></a></li>
                                            </ul>

                                        </div>
                                    </div>
                                    <div class="col-lg-3 col-md-6 col-sm-6">
                                        <div class="address">
                                            <h3>Kurslar</h3>
                                            <ul class="Menu_footer">
                                                <li class="active"> <a href="~/Course/CourseDetails/1">Tasarım</a> </li>
                                                <li><a href="~/Course/CourseDetails/2">Yazılım Geliştirme</a> </li>
                                                <li><a href="~/Course/CourseDetails/3">Pazarlama</a> </li>
                                                <li><a href="~/Course/CourseDetails/4">Kişisel Gelişim</a> </li>
                                                <li><a href="~/Course/CourseDetails/5">İşletme</a> </li>
                                                <li><a href="~/Course/CourseDetails/6">Fotoğrafçılık</a> </li>
                                            </ul>
                                        </div>
                                    </div>

                                    <div class="col-lg-3 col-md-6 col-sm-6 ">
                                        <div class="address">
                                            <a href="~/Default/Index"> <img src="~/Content/images/edulogo.png" alt="logo"></a>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>

                    </div>
                    <div class="copyright">
                        <div class="container">
                            <p>Copyright © 2021 Design by <a href="#">Mehmet Arslan </a></p>
                        </div>
                    </div>
                </div>
            </footr>
            <!-- end footer -->
            <!-- Javascript files-->
            <script src="~/Content/js/jquery.min.js"></script>
            <script src="~/Content/js/popper.min.js"></script>
            <script src="~/Content/js/bootstrap.bundle.min.js"></script>
