            <script src="~/Content/js/custom.js"></script>
            <script src="https:cdnjs.cloudflare.com/ajax/libs/fancybox/2.1.5/jquery.fancybox.min.js"></script>
            <!--====== Javascripts & Jquery ======-->

            <script src="~/Content/style/sepet.js"></script>
            <script src="~/Content/style/js/bootstrap.min.js"></script>
            <script src="~/Content/style/js/mixitup.min.js"></script>
            <script src="~/Content/style/js/circle-progress.min.js"></script>
            <script src="~/Content/style/js/owl.carousel.min.js"></script>
            <script src="~/Content/style/js/main.js"></script>


</body>

</html>