@model IEnumerable<OnlineEgitimProjesi.Models.Ogrenciler>


<table class="table">
    <tr>
        <th>
            Öğrenci Adı
        </th>
        <th>
            Öğrenci Soyad 
        </th>
        <th>
            Öğrenci Eposta
        </th>
        <th>
            Öğrenci Telefon
        </th>
        <th>
            Kurs Adı
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.ogr_ad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ogr_soyad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ogr_eposta)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ogr_sifre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ogr_tel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Videolar.vid_ad)
        </td>
    </tr>
}

</table>
